{
  "name": "studyvibe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "upload-assets": "npx tsx --require dotenv/config scripts/upload-assets.ts",
    "add-background": "npx tsx --require dotenv/config scripts/add-background.ts",
    "add-track": "npx tsx --require dotenv/config scripts/add-track.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "next": "14",
    "react": "^18",
    "react-dom": "^18",
    "three": "^0.183.1",
    "tone": "^15.1.22",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "pnpm": {
    "overrides": {
      "glob@<10.4.0": ">=10.4.5",
      "minimatch@<10.2.1": ">=10.2.1"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/three": "^0.183.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv": "^17.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "jsdom": "^28.1.0",
    "postcss": "^8",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
